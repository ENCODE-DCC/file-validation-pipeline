{
  "name": "fastqc",
  "title": "FastQC",
  "summary": "FastQC command line for ENCODE Universal Pipelines",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "categories": [
    "Encode"
  ],
  "inputSpec": [
    {
      "name": "files",
      "label": "FastQ Files",
      "class": "array:file",
      "optional": false
    }
  ],
  "outputSpec": [
    {
      "name": "reports",
      "label": "Full reports from FastQC",
      "class": "array:file"
    },
    {
      "name": "zips",
      "label": "Zipped reports from FastQC",
      "class": "array:file"
    },
    {
      "name": "summaries",
      "label": "Summaries of FastQC output",
      "class": "array:file"
    }
  ],
  "runSpec": {
    "interpreter": "python2.7",
    "file": "src/fastqc.py",
    "execDepends": [
      {"name":"openjdk-6-jre"}
    ]
  },
  "authorizedUsers": []
}
